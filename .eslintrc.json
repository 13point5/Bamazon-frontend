{
  "extends": [
    "airbnb",
    "prettier",
    "prettier/react",
    "plugin:jsx-a11y/recommended"
  ],
  "parser": "babel-eslint",
  "env": {
    "browser": true,
    "node": true,
    "es6": true
  },
  "plugins": ["prettier", "react", "react-hooks"],
  "rules": {
    "import/no-unresolved": "off",
    "import/prefer-default-export": "off"
  }
}
